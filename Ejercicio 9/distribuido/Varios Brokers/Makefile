C=gcc
CC=g++
CFLAGS=-Wall -pedantic -pedantic-errors -g -ggdb -DDEBUG
LDFLAGS=
BINF=bin
BINF_TCP=bin/tcp
HEADER_DIR=.

SHARED_SOURCES := $(shell find ./*/ -name '[^br|Br|identificador]*.cpp')
SHARED_OBJ=$(SHARED_SOURCES:.cpp=.o)

SHARED_SOURCES_C := $(shell find ./*/ -name '*clnt.c')
SHARED_OBJ_C=$(SHARED_SOURCES_C:.c=.o)

SHARED_SOURCES_SERVER_C := $(shell find ./identificador/ -name 'identificador_server.c')
SHARED_SOURCES_SERVER_C_OBJ := $(SHARED_SOURCES_SERVER_C:.c=.o)

SHARED_SOURCES_ID_SERVER := $(shell find './identificador/' -name '*.cpp')
SHARED_OBJ_ID_SERVER := $(SHARED_SOURCES_ID_SERVER:.cpp=.o)

SHARED_SOURCES_TCP := $(shell find ./logger/ ./sync/ -name '*.cpp')
SHARED_OBJ_TCP=$(SHARED_SOURCES_TCP:.cpp=.o)
	
SHARED_SOURCES_BROKER := $(shell find ./logger/ ./ipc/ ./sync/ -name '*.cpp')
SHARED_OBJ_BROKER=$(SHARED_SOURCES_BROKER:.cpp=.o)

INIT_SOURCES=iniciador.cpp
INIT_OBJ=$(INIT_SOURCES:.cpp=.o)
INIT_BIN=$(BINF)/iniciador

INIT_DISP_SOURCES=iniciadorDisp.cpp
INIT_DISP_OBJ=$(INIT_DISP_SOURCES:.cpp=.o)
INIT_DISP_BIN=$(BINF)/iniciadorDisp

INIT_EQ_ESP_SOURCES=iniciadorEquipoEspecial.cpp
INIT_EQ_ESP_OBJ=$(INIT_EQ_ESP_SOURCES:.cpp=.o)
INIT_EQ_ESP_BIN=$(BINF)/iniciadorEquipoEspecial

FIN_EQ_ESP_SOURCES=terminadorEquipoEspecial.cpp
FIN_EQ_ESP_OBJ=$(FIN_EQ_ESP_SOURCES:.cpp=.o)
FIN_EQ_ESP_BIN=$(BINF)/terminadorEquipoEspecial

FIN_DISP_SOURCES=terminadorDisp.cpp
FIN_DISP_OBJ=$(FIN_DISP_SOURCES:.cpp=.o)
FIN_DISP_BIN=$(BINF)/terminadorDisp

DISP_SOURCES=Dispositivo.cpp
DISP_OBJ=$(DISP_SOURCES:.cpp=.o)
DISP_BIN=$(BINF)/dispositivo

DISPCONFIG_SOURCES=DispositivoConfig.cpp
DISPCONFIG_OBJ=$(DISPCONFIG_SOURCES:.cpp=.o)
DISPCONFIG_BIN=$(BINF)/dispositivoConfig

TESTERCOMUN_SOURCES=TesterComun.cpp
TESTERCOMUN_OBJ=$(TESTERCOMUN_SOURCES:.cpp=.o)
TESTERCOMUN_BIN=$(BINF)/testerComun

TESTERESP_SOURCES=TesterEspecial.cpp
TESTERESP_OBJ=$(TESTERESP_SOURCES:.cpp=.o)
TESTERESP_BIN=$(BINF)/testerEspecial

TECNICO_SOURCES=Tecnico.cpp
TECNICO_OBJ=$(TECNICO_SOURCES:.cpp=.o)
TECNICO_BIN=$(BINF)/tecnico

EQUIPOESP_SOURCES=EquipoEspecial.cpp
EQUIPOESP_OBJ=$(EQUIPOESP_SOURCES:.cpp=.o)
EQUIPOESP_BIN=$(BINF)/equipoEspecial

FIN_SOURCES=terminador.cpp
FIN_OBJ=$(FIN_SOURCES:.cpp=.o)
FIN_BIN=$(BINF)/terminador

INIT_ID_SERVER_SOURCES=./identificador/iniciador_identificador_server.cpp
INIT_ID_SERVER_OBJ=$(INIT_ID_SERVER_SOURCES:.cpp=.o)
INIT_ID_SERVER_BIN=$(BINF)/iniciadorIdServer

ID_SERVER_SOURCES=./identificador/identificador_svc.c
ID_SERVER_OBJ=$(ID_SERVER_SOURCES:.c=.o)
ID_SERVER_BIN=$(BINF)/idServer

FIN_ID_SERVER_SOURCES=./identificador/terminador_identificador_server.cpp
FIN_ID_SERVER_OBJ=$(FIN_ID_SERVER_SOURCES:.cpp=.o)
FIN_ID_SERVER_BIN=$(BINF)/terminadorIdServer

DISTR_MSG_TESTER_COMUN_SOURCES=DistribuidorMensajesTestersComunes.cpp
DISTR_MSG_TESTER_COMUN_OBJ=$(DISTR_MSG_TESTER_COMUN_SOURCES:.cpp=.o)
DISTR_MSG_TESTER_COMUN_BIN=$(BINF)/distribuidorMsgTester

############## BROKER #################
BROKER_SOURCES=broker/Broker.cpp
BROKER_OBJ=$(BROKER_SOURCES:.cpp=.o)
BROKER_BIN=$(BINF)/initBroker

BROKER_PASA_MANOS_EMISOR_SOURCES=broker/brokerPasamanosEmisor.cpp
BROKER_PASA_MANOS_EMISOR_OBJ=$(BROKER_PASA_MANOS_EMISOR_SOURCES:.cpp=.o)
BROKER_PASA_MANOS_EMISOR_BIN=$(BINF)/brokerPasamanosEmisor

BROKER_PASA_MANOS_RECEPTOR_SOURCES=broker/brokerPasamanosReceptor.cpp
BROKER_PASA_MANOS_RECEPTOR_OBJ=$(BROKER_PASA_MANOS_RECEPTOR_SOURCES:.cpp=.o)
BROKER_PASA_MANOS_RECEPTOR_BIN=$(BINF)/brokerPasamanosReceptor

BROKER_REQ_DISP_SOURCES=broker/brokerRequerimientosDisp.cpp
BROKER_REQ_DISP_OBJ=$(BROKER_REQ_DISP_SOURCES:.cpp=.o)
BROKER_REQ_DISP_BIN=$(BINF)/brokerRequerimientosDisp

BROKER_REQ_TEST_ESP_SOURCES=broker/brokerReqEspeciales.cpp
BROKER_REQ_TEST_ESP_OBJ=$(BROKER_REQ_TEST_ESP_SOURCES:.cpp=.o)
BROKER_REQ_TEST_ESP_BIN=$(BINF)/brokerReqTestEsp

BROKER_REGISTRO_TESTERS_SOURCES=broker/brokerRegistroTesters.cpp
BROKER_REGISTRO_TESTERS_OBJ=$(BROKER_REGISTRO_TESTERS_SOURCES:.cpp=.o)
BROKER_REGISTRO_TESTERS_BIN=$(BINF)/brokerRegistroTesters

INTER_BROKER_MSG_HANDLER_SOURCES=broker/interBrokerMsgHandler.cpp
INTER_BROKER_MSG_HANDLER_OBJ=$(INTER_BROKER_MSG_HANDLER_SOURCES:.cpp=.o)
INTER_BROKER_MSG_HANDLER_BIN=$(BINF)/interBrokerMsgHandler

BROKER_SHM_PASSING_HANDLER_SOURCES=broker/brokerShMemPassingHandler.cpp
BROKER_SHM_PASSING_HANDLER_OBJ=$(BROKER_SHM_PASSING_HANDLER_SOURCES:.cpp=.o)
BROKER_SHM_PASSING_HANDLER_BIN=$(BINF)/brokerShMemPassingHandler

BROKER_SHM_TESTER_PASSING_HANDLER_SOURCES=broker/brokerShMemTesterPassingHandler.cpp
BROKER_SHM_TESTER_PASSING_HANDLER_OBJ=$(BROKER_SHM_TESTER_PASSING_HANDLER_SOURCES:.cpp=.o)
BROKER_SHM_TESTER_PASSING_HANDLER_BIN=$(BINF)/brokerShMemTesterPassingHandler

BROKER_SHM_TESTER_REQ_HANDLER_SOURCES=broker/brokerShMemTesterReqHandler.cpp
BROKER_SHM_TESTER_REQ_HANDLER_OBJ=$(BROKER_SHM_TESTER_REQ_HANDLER_SOURCES:.cpp=.o)
BROKER_SHM_TESTER_REQ_HANDLER_BIN=$(BINF)/brokerShMemTesterReqHandler

BROKER_FIN_SOURCES=broker/brokerFin.cpp
BROKER_FIN_OBJ=$(BROKER_FIN_SOURCES:.cpp=.o)
BROKER_FIN_BIN=$(BINF)/finBroker

############## ANILLO #################
RING_SENDER_SOURCES=Anillo/sender.c
RING_SENDER_OBJ=$(RING_SENDER_SOURCES:.c=.o)
RING_SENDER_BIN=$(BINF)/anillo/sender

RING_LISTENER_SOURCES=Anillo/listener.c
RING_LISTENER_OBJ=$(RING_LISTENER_SOURCES:.c=.o)
RING_LISTENER_BIN=$(BINF)/anillo/listener

RING_TIMEOUT_SOURCES=Anillo/timeout.c
RING_TIMEOUT_OBJ=$(RING_TIMEOUT_SOURCES:.c=.o)
RING_TIMEOUT_BIN=$(BINF)/anillo/timeout
#######################################

############## TCP ####################
TCP_SERVER_EMISOR_SOURCES=tcp/tcpserver_emisor.cpp
TCP_SERVER_EMISOR_OBJ=$(TCP_SERVER_EMISOR_SOURCES:.cpp=.o)
TCP_SERVER_EMISOR_BIN=$(BINF_TCP)/tcpserver_emisor

TCP_SERVER_RECEPTOR_SOURCES=tcp/tcpserver_receptor.cpp
TCP_SERVER_RECEPTOR_OBJ=$(TCP_SERVER_RECEPTOR_SOURCES:.cpp=.o)
TCP_SERVER_RECEPTOR_BIN=$(BINF_TCP)/tcpserver_receptor

TCP_CLIENT_EMISOR_SOURCES=tcp/tcpclient_emisor.cpp
TCP_CLIENT_EMISOR_OBJ=$(TCP_CLIENT_EMISOR_SOURCES:.cpp=.o)
TCP_CLIENT_EMISOR_BIN=$(BINF_TCP)/tcpclient_emisor

TCP_CLIENT_RECEPTOR_SOURCES=tcp/tcpclient_receptor.cpp
TCP_CLIENT_RECEPTOR_OBJ=$(TCP_CLIENT_RECEPTOR_SOURCES:.cpp=.o)
TCP_CLIENT_RECEPTOR_BIN=$(BINF_TCP)/tcpclient_receptor
####################################################

all: testerComun testerEspecial tecnico dispositivo equipoEspecial iniciador iniciadorEquipoEspecial terminadorEquipoEspecial terminador iniciadorDisp terminadorDisp iniciadorIdServer idServer terminadorIdServer distribuidorMsgTester initBroker brokerPasamanosEmisor brokerPasamanosReceptor brokerRequerimientosDisp brokerReqTestEsp brokerRegTesters interBrokerMsgHandler brokerShmemPassingHandler brokerShmemTesterPassingHandler brokerShMemTesterReqHandler finBroker anillo tcpServerEmisor tcpServerReceptor tcpClientEmisor tcpClientReceptor

##################################### GENERAL STUFF #########################################

iniciador: $(INIT_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(INIT_OBJ) $(SHARED_OBJ_C) -o $(INIT_BIN) -I$(HEADER_DIR)

dispositivo: $(DISP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(DISP_OBJ) $(SHARED_OBJ_C) -o $(DISP_BIN) -I$(HEADER_DIR)
	
dispositivoConfig: $(DISPCONFIG_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(DISP_OBJ) $(SHARED_OBJ_C) -o $(DISPCONFIG_BIN) -I$(HEADER_DIR)
	
testerComun: $(TESTERCOMUN_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(TESTERCOMUN_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C) -o $(TESTERCOMUN_BIN) -I$(HEADER_DIR)
	
testerEspecial: $(TESTERESP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(TESTERESP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C) -o $(TESTERESP_BIN) -I$(HEADER_DIR)
	
equipoEspecial: $(EQUIPOESP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(EQUIPOESP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C) -o $(EQUIPOESP_BIN) -I$(HEADER_DIR)

tecnico: $(TECNICO_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(TECNICO_OBJ) $(SHARED_OBJ_C) -o $(TECNICO_BIN) -I$(HEADER_DIR)

terminador: $(FIN_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(FIN_OBJ) $(SHARED_OBJ_C) -o $(FIN_BIN) -I$(HEADER_DIR)

iniciadorIdServer: $(INIT_ID_SERVER_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(INIT_ID_SERVER_OBJ) -o $(INIT_ID_SERVER_BIN) -I$(HEADER_DIR)

idServer: $(ID_SERVER_OBJ) $(SHARED_SOURCES_SERVER_C_OBJ)
	$(CC) $(SHARED_SOURCES_SERVER_C_OBJ) $(ID_SERVER_OBJ) -o $(ID_SERVER_BIN) -I$(HEADER_DIR)

terminadorIdServer: $(FIN_ID_SERVER_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(FIN_ID_SERVER_OBJ) -o $(FIN_ID_SERVER_BIN) -I$(HEADER_DIR)

iniciadorDisp: $(INIT_DISP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(INIT_DISP_OBJ) -o $(INIT_DISP_BIN) -I$(HEADER_DIR)

terminadorDisp: $(FIN_DISP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(FIN_DISP_OBJ) -o $(FIN_DISP_BIN) -I$(HEADER_DIR)

iniciadorEquipoEspecial: $(INIT_EQ_ESP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(INIT_EQ_ESP_OBJ) -o $(INIT_EQ_ESP_BIN) -I$(HEADER_DIR)

terminadorEquipoEspecial: $(FIN_EQ_ESP_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(FIN_EQ_ESP_OBJ) -o $(FIN_EQ_ESP_BIN) -I$(HEADER_DIR)

distribuidorMsgTester: $(DISTR_MSG_TESTER_COMUN_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(DISTR_MSG_TESTER_COMUN_OBJ) -o $(DISTR_MSG_TESTER_COMUN_BIN) -I$(HEADER_DIR)

################################## FIN-GENERAL-STUFF #####################################

##################################### BROKER #############################################

initBroker: $(BROKER_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_OBJ) -o $(BROKER_BIN) -I$(HEADER_DIR)

brokerPasamanosEmisor: $(BROKER_PASA_MANOS_EMISOR_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_PASA_MANOS_EMISOR_OBJ) -o $(BROKER_PASA_MANOS_EMISOR_BIN) -I$(HEADER_DIR)

brokerPasamanosReceptor: $(BROKER_PASA_MANOS_RECEPTOR_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_PASA_MANOS_RECEPTOR_OBJ) -o $(BROKER_PASA_MANOS_RECEPTOR_BIN) -I$(HEADER_DIR)

brokerRequerimientosDisp: $(BROKER_REQ_DISP_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_REQ_DISP_OBJ) -o $(BROKER_REQ_DISP_BIN) -I$(HEADER_DIR)

brokerReqTestEsp: $(BROKER_REQ_TEST_ESP_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_REQ_TEST_ESP_OBJ) -o $(BROKER_REQ_TEST_ESP_BIN) -I$(HEADER_DIR)

brokerRegTesters: $(BROKER_REGISTRO_TESTERS_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_REGISTRO_TESTERS_OBJ) -o $(BROKER_REGISTRO_TESTERS_BIN) -I$(HEADER_DIR)

interBrokerMsgHandler: $(INTER_BROKER_MSG_HANDLER_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(INTER_BROKER_MSG_HANDLER_OBJ) -o $(INTER_BROKER_MSG_HANDLER_BIN) -I$(HEADER_DIR)

brokerShmemPassingHandler: $(BROKER_SHM_PASSING_HANDLER_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_SHM_PASSING_HANDLER_OBJ) -o $(BROKER_SHM_PASSING_HANDLER_BIN) -I$(HEADER_DIR)

brokerShmemTesterPassingHandler: $(BROKER_SHM_TESTER_PASSING_HANDLER_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_SHM_TESTER_PASSING_HANDLER_OBJ) -o $(BROKER_SHM_TESTER_PASSING_HANDLER_BIN) -I$(HEADER_DIR)

brokerShMemTesterReqHandler: $(BROKER_SHM_TESTER_REQ_HANDLER_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_SHM_TESTER_REQ_HANDLER_OBJ) -o $(BROKER_SHM_TESTER_REQ_HANDLER_BIN) -I$(HEADER_DIR)

finBroker: $(BROKER_FIN_OBJ) $(SHARED_OBJ_BROKER)
	$(CC) $(SHARED_OBJ_BROKER) $(BROKER_FIN_OBJ) -o $(BROKER_FIN_BIN) -I$(HEADER_DIR)

################################  FIN-BROKER  #################################################

################################  ANILLO  #####################################################

anillo: $(RING_SENDER_OBJ) $(RING_LISTENER_OBJ) $(RING_TIMEOUT_OBJ) $(SHARED_OBJ) $(SHARED_OBJ_C)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(RING_SENDER_OBJ) -o $(RING_SENDER_BIN) -I$(HEADER_DIR)
	$(CC) $(SHARED_OBJ) $(SHARED_OBJ_C) $(RING_LISTENER_OBJ) -o $(RING_LISTENER_BIN) -I$(HEADER_DIR)
	$(CC) $(RING_TIMEOUT_OBJ) -o $(RING_TIMEOUT_BIN) -I$(HEADER_DIR)

############################### FIN-ANILLO ####################################################

tcpServerEmisor: $(TCP_SERVER_EMISOR_OBJ) $(SHARED_OBJ_TCP)
	$(CC) $(SHARED_OBJ_TCP) $(TCP_SERVER_EMISOR_OBJ) -o $(TCP_SERVER_EMISOR_BIN) -I$(HEADER_DIR)

tcpServerReceptor: $(TCP_SERVER_RECEPTOR_OBJ) $(SHARED_OBJ_TCP)
	$(CC) $(SHARED_OBJ_TCP) $(TCP_SERVER_RECEPTOR_OBJ) -o $(TCP_SERVER_RECEPTOR_BIN) -I$(HEADER_DIR)

tcpClientEmisor: $(TCP_CLIENT_EMISOR_OBJ) $(SHARED_OBJ_TCP)
	$(CC) $(SHARED_OBJ_TCP) $(TCP_CLIENT_EMISOR_OBJ) -o $(TCP_CLIENT_EMISOR_BIN) -I$(HEADER_DIR)

tcpClientReceptor: $(TCP_CLIENT_RECEPTOR_OBJ) $(SHARED_OBJ_TCP)
	$(CC) $(SHARED_OBJ_TCP) $(TCP_CLIENT_RECEPTOR_OBJ) -o $(TCP_CLIENT_RECEPTOR_BIN) -I$(HEADER_DIR)

.cpp.o:
	$(CC) $(CFLAGS) -I$(HEADER_DIR) -c $< -o $@
	
.c.o:
	$(CC) $(CFLAGS) -I$(HEADER_DIR) -c $< -o $@

clean:
	# REMOVE GENERAL STUFF
	$(RM) $(INIT_OBJ) $(INIT_BIN) $(DISP_OBJ) $(DISP_BIN) $(TESTERCOMUN_OBJ) $(TESTERCOMUN_BIN) $(TESTERESP_OBJ) $(TESTERESP_BIN) $(EQUIPOESP_OBJ) $(EQUIPOESP_BIN) $(TECNICO_OBJ) $(TECNICO_BIN) $(FIN_BIN) $(FIN_OBJ) $(INIT_ID_SERVER_BIN) $(INIT_ID_SERVER_OBJ) $(ID_SERVER_BIN) $(ID_SERVER_OBJ) $(FIN_ID_SERVER_BIN) $(FIN_ID_SERVER_OBJ) $(INIT_DISP_OBJ) $(INIT_DISP_BIN) $(FIN_DISP_OBJ) $(FIN_DISP_BIN) $(SHARED_OBJ) $(SHARED_OBJ_C) $(SHARED_SOURCES_SERVER_C_OBJ) $(DISTR_MSG_TESTER_COMUN_OBJ) $(DISTR_MSG_TESTER_COMUN_BIN) $(INIT_EQ_ESP_OBJ) $(INIT_EQ_ESP_BIN) $(FIN_EQ_ESP_OBJ) $(FIN_EQ_ESP_BIN) $(RING_SENDER_OBJ) $(RING_SENDER_BIN) $(RING_LISTENER_OBJ) $(RING_LISTENER_BIN) $(RING_TIMEOUT_OBJ) $(RING_TIMEOUT_BIN)
	# REMOVE TCP STUFF
	$(RM) $(TCP_SERVER_EMISOR_OBJ) $(TCP_SERVER_EMISOR_BIN) $(TCP_SERVER_RECEPTOR_OBJ) $(TCP_SERVER_RECEPTOR_BIN) $(TCP_CLIENT_EMISOR_OBJ) $(TCP_CLIENT_EMISOR_BIN) $(TCP_CLIENT_RECEPTOR_OBJ) $(TCP_CLIENT_RECEPTOR_BIN)
	# REMOVE BROKER STUFF
	$(RM) $(BROKER_OBJ) $(BROKER_BIN) $(BROKER_PASA_MANOS_EMISOR_OBJ) $(BROKER_PASA_MANOS_EMISOR_BIN) $(BROKER_PASA_MANOS_RECEPTOR_OBJ) $(BROKER_PASA_MANOS_RECEPTOR_BIN) $(BROKER_REQ_DISP_OBJ) $(BROKER_REQ_DISP_BIN) $(BROKER_REQ_TEST_ESP_OBJ) $(BROKER_REQ_TEST_ESP_BIN) $(BROKER_REGISTRO_TESTERS_OBJ) $(BROKER_REGISTRO_TESTERS_BIN) $(INTER_BROKER_MSG_HANDLER_OBJ) $(INTER_BROKER_MSG_HANDLER_BIN) $(BROKER_SHM_PASSING_HANDLER_OBJ) $(BROKER_SHM_PASSING_HANDLER_BIN) $(BROKER_SHM_TESTER_PASSING_HANDLER_OBJ) $(BROKER_SHM_TESTER_PASSING_HANDLER_BIN) $(BROKER_SHM_TESTER_REQ_HANDLER_OBJ) $(BROKER_SHM_TESTER_REQ_HANDLER_BIN) $(BROKER_FIN_OBJ) $(BROKER_FIN_BIN)